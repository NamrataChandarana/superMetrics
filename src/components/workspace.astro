---
import { workspaceImages } from "../content/allImges"
---

<div class="p-page-padding py-20 text-center border-b border-gray-300">
    <div class="w-full">
        <div class="pb-12 h-32 w-[602px] text-wrap mx-auto ">
            <h1 class="text-center text-darkBlue text-4xl  font-semibold ">Access all your marketing data without leaving</h1>
            <img src="../assets/google.png" alt="">
        </div>
        <div class="flex gap-[-3em]">
            {workspaceImages.map(({src, alt}: {src: string, alt: string}) => (
                <img src={src} alt={alt} />
            ))}
        </div>
    </div>
</div>